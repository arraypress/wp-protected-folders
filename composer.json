{
  "name": "arraypress/wp-protected-folders",
  "version": "1.0.0",
  "description": "Simple folder protection for WordPress with automatic upload organization and .htaccess management.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-protected-folders",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\ProtectedFolders\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-protected-folders/issues",
    "source": "https://github.com/arraypress/wp-protected-folders"
  },
  "keywords": [
    "wordpress",
    "wp",
    "file-protection",
    "folder-protection",
    "upload-protection",
    "htaccess",
    "file-security",
    "directory-protection",
    "download-protection",
    "protected-folders",
    "arraypress"
  ]
}